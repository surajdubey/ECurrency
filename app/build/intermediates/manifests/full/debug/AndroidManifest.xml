<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ecurrency.com.ecurreny"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- Network State Permissions -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ecurrency.com.ecurreny.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ecurrency.com.ecurreny.RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
        <activity
            android:name="ecurrency.com.ecurreny.WelcomeActivity"
            android:label="@string/title_activity_welcome" >
        </activity>
        <activity
            android:name="ecurrency.com.ecurreny.PaymentActivity"
            android:label="@string/title_activity_payment" >
        </activity>
        <activity
            android:name="ecurrency.com.ecurreny.ProcessTransaction"
            android:label="@string/title_activity_process_transaction" >
        </activity>
        <activity
            android:name="ecurrency.com.ecurreny.ReceiveActivity"
            android:label="@string/title_activity_receive" >
        </activity>
        <activity
            android:name="ecurrency.com.ecurreny.AcceptMoneyActivity"
            android:label="@string/title_activity_accept_money" >
        </activity>
        <activity
            android:name="ecurrency.com.ecurreny.TransactionListActivity"
            android:label="@string/title_activity_transaction_list" >
        </activity>
    </application>

</manifest>